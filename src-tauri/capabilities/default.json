{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:read-all",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:write-all",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:deny-webview-data-windows"
    },
    {
      "identifier": "fs:deny-webview-data-linux"
    },
    {
      "identifier": "fs:allow-fstat",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-ftruncate",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-copy-file",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-remove",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-rename",
      "allow": [
        "**"
      ]
    },
    "os:default",
    "os:allow-arch",
    "os:allow-hostname",
    "os:allow-os-type"
  ]
}